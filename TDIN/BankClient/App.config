<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown type="AuthenticationObj, Program" url="tcp://localhost:9000/Server/Bank.rem" />
