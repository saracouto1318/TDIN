<configuration>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown type="IUser, Shared" url="tcp://localhost:9000/Server/User.rem" />
        <wellknown type="ITransaction, Shared" url="tcp://localhost:9000/Server/Transaction.rem" />
      </client>
      <channels>
        <channel ref="tcp" port="0">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>